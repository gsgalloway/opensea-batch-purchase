    "compile": "READABLE_STREAM=disable yarn hardhat compile",
    "test": "yarn hardhat test",
    "rollup": "rollup -c",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "hardhat": "READABLE_STREAM=disable TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ./node_modules/.bin/hardhat"
  },
  "engines": {
    "node": ">= 14"
  },
  "description": "Utility for batch purchases of assets on OpenSea",
