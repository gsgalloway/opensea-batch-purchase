{
    "name": "@standard-crypto/opensea-batch-purchaser-openapi",
    "version": "1.0.0",
    "files": [
        "generated"
    ],
    "private": true,
    "scripts": {
        "validate": "swagger-cli validate ./swagger.yaml",
        "generate": "yarn validate && yarn generate:types && yarn generate:server",
        "generate:types": "openapi-generator-cli generate -i swagger.yaml -o dist/types -g typescript-node",
        "generate:server": "openapi-generator-cli generate -i swagger.yaml -o dist/server -g nodejs-express-server"
    },
    "devDependencies": {
        "@apidevtools/swagger-cli": "^4.0.4",
        "@openapitools/openapi-generator-cli": "^2.4.21"
    },
    "dependencies": {
        "@types/request": "^2.48.7",
        "openapi-types": "^10.0.0"
    }
}
