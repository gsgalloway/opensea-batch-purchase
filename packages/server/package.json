{
  "name": "server",
  "version": "0.0.1",
  "scripts": {
    "tsc": "./node_modules/.bin/tsc",
    "build": "rimraf ./dist/**/* && yarn tsc --skipLibCheck",
    "build-bak": "rimraf ./dist/**/* && rollup -c rollup.config.js",
    "start": "functions-framework --source=dist/ --target=app",
    "dev": "ts-node-dev --respawn --pretty src/index.ts",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./dist/ --exec npm run start\""
  },
  "dependencies": {
    "@ethersproject/providers": "^5.5.1",
    "@standard-crypto/opensea-batch-purchaser": "0.0.10",
    "@standard-crypto/opensea-batch-purchaser-openapi": "1.0.0",
    "electron": "^16.0.5",
    "ethers-multisend": "^2.0.1",
    "opensea-js": "^1.2.7",
    "readable-stream": "npm:vite-compatible-readable-stream"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^2.1.0",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-commonjs": "^22.0.0-4",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/express": "^4.17.13",
    "concurrently": "^6.5.1",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-externals": "^3.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
